{
    "builds": [
        {
            "src": "backend/config/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12",
                "pip": ["django", "-r backend/requirements.txt"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "backend/config/wsgi.py"
        }
    ]
}
